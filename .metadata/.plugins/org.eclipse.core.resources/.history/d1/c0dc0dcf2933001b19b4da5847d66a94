package alone_java_CH09_EX2_2;

public class Anonymous {
	Vehicle field = new Vehicle() {
		@Override
		public void run() {
			System.out.println("자전거가 달립니다.");
		}
	};
	
	void method1() {
		Vehicle localVar = new Vehicle() {
			@Override
			public void run() {
				System.out.println("개발을 합니다.");
			}
		};
		localVar.run();
		
	}
	void method2(Vehicle vehicle) {
		vehicle.run();
	}
}

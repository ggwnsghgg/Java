package alone_java_CH09_EX2_2;

public class AnnoymousExample {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Anonymous anony = new Anonymous();
		anony.field.start();
		anony.method1();
		anony.method2(
				new Vehicle() {
					@Override
					public void start() {
						System.out.println("테스트를 합니다.");
					}
				}
			);
	}

}

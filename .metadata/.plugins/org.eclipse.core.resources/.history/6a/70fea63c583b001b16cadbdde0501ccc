
package pos;

import javax.swing.*;
import java.awt.*;

 class main extends JFrame{
	private static Image img;
	public main(Image img) {
		this.img = img;
		setSize(new Dimension(img.getWidth(null),img.getHeight(null)))
	}

	public static void paintComponent (Graphics g) {
		g.drawImage(img, 0,0, null);
	}

}

package sec01.exam07;

public class Calculator {
	private int memory;

	public int getMemory() {
		return this.memory;
	}

	public void setMemory(int memory) {
		this.memory = memory;

		try {
			Thread.sleep(2000L);
		} catch (InterruptedException var3) {
			;
		}

		System.out.println(Thread.currentThread().getName() + ": " + this.memory);
	}
}
package pos;

import javax.swing.*;
import java.awt.*;

public class main {
		public static void mian(String[] args) {
			JFrame frm = new JFrame();
			JPanel pan = new JPanel();
			
			
			frm.add(pan);
			frm.setResizable(false);
			frm.setPreferredSize(new Dimension(800,700));
			frm.setSize(800,700);
			frm.setVisible(true);
			frm.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
	}
}

package innerclass;


class Outer2 {
	Runnable getRunnable(int i) {
		int num = 100;
		
		return new Runnable() {

			@Override
			public void run() {
				// TODO Auto-generated method stub
				//num = 200;
				// i = 10;
				System.out.println(i);
				System.out.println(num);
			}
			
		};
	}
	Runnable runner = new Runnable() {
		
		@Override
		public void run() {
			// TODO Auto-generated method stub
			System.out.println("Runnable이 구현된 익명 클래스 변수");
			
		}
	};
}

